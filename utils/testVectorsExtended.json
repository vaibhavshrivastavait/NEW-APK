  "testCases": {
    "bmi": [
      {
        "name": "Normal BMI - Average Adult",
        "description": "70kg, 175cm adult - should be normal BMI",
        "input": {
          "weight": 70,
          "height": 175
        },
        "expectedBMI": 22.9,
        "expectedCategory": "Normal",
        "expectedHealthRisk": "Low",
        "tolerance": 0.1,
        "notes": "Should calculate to 22.9 BMI (normal weight)"
      },
      {
        "name": "Underweight BMI",
        "description": "45kg, 170cm adult - underweight",
        "input": {
          "weight": 45,
          "height": 170
        },
        "expectedBMI": 15.6,
        "expectedCategory": "Underweight",
        "expectedHealthRisk": "Moderate",
        "tolerance": 0.1,
        "notes": "Should be classified as underweight"
      },
      {
        "name": "Overweight BMI",
        "description": "80kg, 165cm adult - overweight",
        "input": {
          "weight": 80,
          "height": 165
        },
        "expectedBMI": 29.4,
        "expectedCategory": "Overweight",
        "expectedHealthRisk": "Moderate",
        "tolerance": 0.1,
        "notes": "Should be classified as overweight"
      },
      {
        "name": "Obese Class I",
        "description": "95kg, 170cm adult - obese class I",
        "input": {
          "weight": 95,
          "height": 170
        },
        "expectedBMI": 32.9,
        "expectedCategory": "Obese Class I",
        "expectedHealthRisk": "High",
        "tolerance": 0.1,
        "notes": "Should be classified as obese class I"
      },
      {
