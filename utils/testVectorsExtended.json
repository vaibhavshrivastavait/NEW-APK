  "validationCriteria": {
    "bmi": {
      "weightRange": [1, 500],
      "heightRange": [50, 250],
      "bmiRange": [10, 70],
      "categories": ["Underweight", "Normal", "Overweight", "Obese Class I", "Obese Class II", "Obese Class III"],
      "categoryThresholds": [18.5, 25, 30, 35, 40]
    },
    "bsa": {
      "weightRange": [1, 500],
      "heightRange": [50, 250],
      "bsaRange": [0.5, 3.0],
      "methods": ["Du Bois Formula", "Mosteller Formula"]
    },
    "egfr": {
      "ageRange": [18, 120],
      "creatinineRange": [0.3, 15.0],
      "egfrRange": [5, 150],
      "stages": ["Normal", "Mild Decrease", "Moderate Decrease", "Severe Decrease", "Kidney Failure"],
      "stageThresholds": [90, 60, 30, 15]
    },
    "hrt": {
      "ageRange": [40, 80],
      "timeSinceMenopauseRange": [0, 30],
      "bmiRange": [15, 50],
      "overallRiskCategories": ["Low", "Moderate", "High"],
      "relativeRiskRange": [0, 5.0]
    }
  },
  "unitConversions": {
    "weight": {
      "lbToKg": 0.453592,
      "kgToLb": 2.20462
    },
    "height": {
      "inToCm": 2.54,
      "cmToIn": 0.393701,
      "ftToIn": 12,
      "cmToFt": 0.0328084
    },
    "creatinine": {
